{
  "name": "aas-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development node ./bin/www",
    "local": "NODE_ENV=local node ./bin/www",
    "prod": "NODE_ENV=production node ./bin/www",
    "test": "NODE_ENV=$ENV mocha",
    "build": "pkg . --out-path ../"
