    "http-errors": "~1.6.3",
    "ibm-watson": "^4.4.0",
    "jade": "~1.11.0",
    "mocha": "^6.2.0",
    "moment-timezone": "^0.5.35",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "request": "^2.88.0",
    "should": "^13.2.3",
    "srt2xmeml": "^0.1.6",
    "subtitle": "^2.0.3",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7",
    "tmp": "^0.1.0",
